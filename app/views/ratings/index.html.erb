<p style="color: green"><%= notice %></p>

<h2>List of ratings</h2>
<p>Number of ratings: <%= @ratings.count %></p>

<div id="ratings">
  <ul>
    <% @ratings.each do |rating| %>
      <li> <%= rating.print_report %></li>
    <% end %>
  </ul>
</div>

<p><%= link_to "Create new rating", new_rating_path %></p>
